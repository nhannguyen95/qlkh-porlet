<service-builder package-path="com.qlkh">
 <author>apple</author>
 <namespace>QLKH</namespace>
<entity name="KhoHang" local-service="true" remote-service="true">
              <!-- PK fields -->
              <column name="MaKho" type="String" primary="true" />
              <column name="TenKho" type="String" />
              <column name="DiaDiem" type="String" />
</entity>

<entity name="HangTrongKho" local-service="true" remote-service="true">
              <!-- PK fields -->
              <column name="MaKho" type="String" primary="true" />
              <column name="MaHang" type="String" primary="true"/>
              <column name="SoLuong" type="long" />
</entity>

<entity name="MatHang" local-service="true" remote-service="true">
              <!-- PK fields -->
              <column name="MaHang" type="String" primary="true" />
              <column name="TenHang" type="String" />
              <column name="GiaThanh" type="String" />
</entity>

</service-builder>